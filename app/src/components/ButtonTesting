import React, { useEffect, useState } from "react";
import axios from 'axios';

function Testing() {
  const [breakfastItems, setBreakfast] = useState([]);

  useEffect(() => {
    axios.get('https://www.jsonkeeper.com/b/MDXW')
      .then(response => {
        setBreakfast(response.data);
      })
      .catch(error => {
        console.log(error);
      });
  }, []);

  const handleClick = () => {
    const filteredItems = breakfastItems.filter(item => item.category === 'Breakfast');
    console.log(filteredItems)
    setBreakfast(filteredItems);
  };

  return 
}
  

export default Testing;